  procedures :
   -
     name : MAJProjet
     procedure_id : 1741196814307202135
     type_code : 14
     code : |1-
      // Résumé : Effectue l'analyse et la synchronisation du projet de synchronisation
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure MAJProjet()
      
      // Variables locales
      tabRepertoireException est un tableau de chaine
      tabExtensionsException est un tableau de chaine
      
      gbSuppression = PROJET.CopieId
      
      // Lecture des exceptions
      ChaîneVersTableau(PROJET.RépertoiresException,tabRepertoireException,RC)
      ChaîneVersTableau(PROJET.FichiersException,tabExtensionsException,RC)
      
      // initialisation des champs
      SAI_REPERTOIRE_SOURCE  = ComplèteRep(PROJET.RepSource)
      SAI_REPERTOIRE_DESTINATION = ComplèteRep(PROJET.RepDest)
      
      // Configure les exceptions
      COL_Synchronisation.DefinirException.tabListeRepertoireException =tabRepertoireException
      COL_Synchronisation.DefinirException.tabListeExtensionsException =tabExtensionsException
      COL_Synchronisation.DefinirException()
      
      // on calcule la synchronisation des deux répertoires	
      COL_Synchronisation.AnalyseRépertoires.sNomRépertoireSource = ComplèteRep(PROJET.RepSource)
      COL_Synchronisation.AnalyseRépertoires.sNomRépertoireDestination = ComplèteRep(PROJET.RepDest)
      COL_Synchronisation.AnalyseRépertoires.bAnalyserSousRepertoire = PROJET.SousRep
      COL_Synchronisation.AnalyseRépertoires.bEcraserFichiersPlusRécents = PROJET.MAJInverse
      SI	COL_Synchronisation.AnalyseRépertoires() = Faux alors
      	Erreur("La comparaison ne peut pas être effectuée",ErreurInfo())
      	RETOUR	
      FIN
      
      // On remplit la table de comparaison
      RemplirTableComparaison()
      
      // copie des fichiers
      MiseAJour()
     type : 458752
   -
     name : CompteRenduSynchronisation
     procedure_id : 1741196814307333207
     type_code : 14
     code : |1+
      // Résumé : Affiche le compte rendu de la synchronisation
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure CompteRenduSynchronisation()
      
      // Variables locales
      sMessage est une chaine
      
      sELON gnFicCopiés
      	CAS 0,1
      		sMessage = ChaîneConstruit(<§$0003§>,gnFicCopiés,gnFicACopier)
      	AUTRE CAS
      		sMessage = ChaîneConstruit(<§$0004§>,gnFicCopiés,gnFicACopier)
      FIN
      
      // mode copie à l'identique
      SI gbSuppression ALORS
      	si gnFicASupprimer>0 alors
      		SELON gnFicSupprimés
      			CAS 0,1
      				sMessage+= ChaîneConstruit(<§$0005§>, gnFicSupprimés, gnFicASupprimer)
      			AUTRE CAS
      				sMessage+= ChaîneConstruit(<§$0006§>, gnFicSupprimés, gnFicASupprimer)
      		fin
      	FIN
      FIN
      
      // Affichage du message	
      Message(sMessage)
     type : 458752
   -
     name : MiseAJour
     procedure_id : 1741196814307398743
     type_code : 14
     code : |1-
      // Résumé : Demande de synchronisation effective
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure MiseAJour()
      
      // Variables locales
      nIndice est un entier
      nLignes est un entier 
      
      // Initialisation du nombre de fichier à traiter
      nLignes = TableOccurrence(TableListe)
      gnFicCopiés = 0
      gnFicSupprimés = 0
      sablier(vrai)
      
      // parcours de la table des fichiers à traités
      POUR nIndice=1 _A_ nLignes
      	// affichage de la jauge
      	Jauge(nIndice,nLignes)
      	
      	// si l'interrupteur est coché
      	SI COL_ACTIF[nIndice] = vrai ALORS
      
      		// si c'est un fichier à copier
      		SI COL_ETAT[nIndice] = CST_FICHIER_A_COPIER ALORS
      			
      			// copie du fichier
      			BOUCLE
      				SI COL_Synchronisation.AppliqueSynchronisationFichier(COL_FICHIER[nIndice]) = Vrai alors					
      					gnFicCopiés++
      					COL_ETAT[nIndice] = CST_FICHIER_COPIE
      					SORTIR
      				FIN
      				
      				SI OuiNon(ChaîneConstruit(<§$0009§>, COL_FICHIER[nIndice]), <§$000a§>, <§$000b§>)=non ALORS 
      					COL_ETAT[nIndice] = CST_FICHIER_COPIE_ECHEC
      					SORTIR
      				FIN	
      			FIN
      		fin	
      			
      		// si c'est un fichier à supprimer	
      		SI COL_ETAT[nIndice] = CST_FICHIER_A_SUPPRIMER  ALORS
      			BOUCLE
      				// suppression du fichier
      				SI COL_Synchronisation.AppliqueSynchronisationFichier(COL_FICHIER[nIndice]) = Vrai ALORS		
      					gnFicSupprimés++
      					COL_ETAT[nIndice] = CST_FICHIER_SUPPRIME
      					SORTIR
      				FIN
      				
      				SI OuiNon(ChaîneConstruit(<§$000f§>,COL_FICHIER[nIndice]), <§$000a§>, <§$000b§>)=non ALORS 
      					COL_ETAT[nIndice] = CST_FICHIER_SUPPRIME_ECHEC
      					SORTIR
      				FIN	
      			FIN
      		FIN
      	
      	// si l'interrupteur n'est pas coché	
      	SINON
      		COL_ETAT[nIndice] = CST_FICHIER_IGNORE
      	FIN
      	
      	FenRepeint()
      FIN
      
      // compte-rendu
      CompteRenduSynchronisation()
      
      Sablier(faux)
