   -
     code : |1+
      CONSTANT
      	CST_FICHIER_A_COPIER = <§$0014§>
      	CST_FICHIER_A_SUPPRIMER = <§$0016§>
      	CST_FICHIER_COPIE = <§$0008§>
      	CST_FICHIER_COPIE_ECHEC = <§$000c§>
      	CST_FICHIER_SUPPRIME = <§$000e§>
      	CST_FICHIER_SUPPRIME_ECHEC = <§$0010§>
      	CST_FICHIER_IGNORE = <§$0011§>
      FIN
      
      global
      	gnFicACopier, gnFicASupprimer sont des entiers // nombre de fichiers à copier et à supprimer
      	gnFicCopiés, gnFicSupprimés sont des entiers // nombre de fichiers copiés et à supprimés
   -
     code : |1+
      // s'il n'y a aucun projet, on ouvre la fenêtre de configuration
      HLitPremier(PROJET,Nom)
      SI HEnDehors(PROJET) ALORS
      	ouvre(FEN_PROJET)
      FIN
      
      // s'il y a une ligne de commande
      si gsLigneCommande<>"" alors
      	
      	// On est en mode automatique
      	gbAuto = vrai
      	DélaiAvantFermeture(500)
      	OuvreFille(FEN_TRAITEMENT_AUTOMATIQUE)
      	
      	si gsLigneCommande<>"*" alors
      		// recherche du projet
      		HLitRecherchePremier(PROJET,Nom,gsLigneCommande)
      		SI HTrouve() ALORS
      			// titre de la fenêtre
      			FEN_TRAITEMENT_AUTOMATIQUE..titre = <§$0000§>+projet.nom
      			FEN_TRAITEMENT_AUTOMATIQUE.Lib_Rep_Source = <§$0001§>+projet.RepSource
      			FEN_TRAITEMENT_AUTOMATIQUE.Lib_Rep_Dest = <§$0002§>+projet.RepDest
      		
      			MAJProjet()
      		fin
      
      	// tous les projets	
      	sinon
      		hlitpremier(PROJET,Nom)
      		tantque pas hendehors(PROJET)
      			// titre de la fenêtre
      			FEN_TRAITEMENT_AUTOMATIQUE..Titre = <§$0000§>+PROJET.Nom
      			FEN_TRAITEMENT_AUTOMATIQUE.Lib_Rep_Source = <§$0001§>+PROJET.RepSource
      			FEN_TRAITEMENT_AUTOMATIQUE.Lib_Rep_Dest = <§$0002§>+PROJET.RepDest
      			MAJProjet()
      			hlitsuivant(PROJET,Nom)
      		FIN
      	FIN
      
      	FinProgramme()
      
      sinon
      	// en mode visuel
      	gbAuto = faux
      	
      	// Est-ce qu'un projet est défini ?
      	SI ListeOccurrence(Combo_PROJET)>0 ALORS
      		// premier projet de la combo
      		ListeSelectPlus(Combo_PROJET,1)
      		HLitRecherchePremier(PROJET,IDPROJET,Combo_PROJET)
      		ChargeProjet()
      	FIN	
      FIN
