   -
     name : CompteRenduListing
     procedure_id : 1741196814307595351
     type_code : 14
     code : |1+
      // Résumé : Affiche le compte-rendu d'analyse de synchronisation
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure CompteRenduListing()
      
      // s'il n'y a aucun fichier à traiter
      SI (gnFicACopier=0 ET gnFicASupprimer=0) ALORS
      	Message(<§$0018§>)
      	SI gbAuto = Faux ALORS
      		Info(<§$0018§>)
      	FIN
      	
      SINON
      	sMessage est une chaine
      	// message à afficher
      	SELON gnFicACopier
      		CAS 0
      			sMessage = <§$0019§>
      		CAS 1
      			sMessage = <§$001a§>
      		AUTRE CAS
      			sMessage = ChaîneConstruit(<§$001b§>,gnFicACopier)
      	FIN
      	
      	// cas copie à l'identique
      	SI gbSuppression ALORS
      		SELON gnFicASupprimer
      			CAS 0
      				sMessage+=""
      			CAS 1	
      				sMessage+=<§$001c§>
      			AUTRE CAS
      				sMessage+= ChaîneConstruit(<§$001d§>, gnFicASupprimer)
      		FIN
      	FIN
      	
      	// affichage message	
      	Message(sMessage)
      
      FIN
     type : 458752
   -
     name : ChargeProjet
     procedure_id : 1741196814307791959
     type_code : 14
     code : |1-
      // Résumé : Charge la configuration du projet de synchronisation
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure ChargeProjet()
      
      gbSousRep = PROJET.SousRep
      gbEcrase = PROJET.MAJInverse
      gbSuppression = PROJET.CopieId
      
      // initialisation des champs
      SAI_REPERTOIRE_SOURCE  = ComplèteRep(PROJET.RepSource)
      SAI_REPERTOIRE_DESTINATION = ComplèteRep(PROJET.RepDest)
     type : 458752
   -
     name : AfficheComparaison
     procedure_id : 1741196814307857495
     type_code : 14
     code : |1+
      // Résumé : Affiche le résultat de la synchronisation
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheComparaison()
      
      // Variables locales
      sFichierEnCours		est une chaine
      
      // Suppression du contenu précédent
      TableSupprimeTout(TableListe)
      
      // On parcours les fichiers à copier
      Pour tout sFichierEnCours de COL_Synchronisation.gtabFichierDestinationACopier
      	TableAjouteLigne(TableListe,Vrai,sFichierEnCours,CST_FICHIER_A_COPIER)
      FIN
      
      // On parcours les fichiers à supprimer
      SI gbSuppression = Vrai alors
      	POUR TOUT sFichierEnCours DE COL_Synchronisation.gtabFichierDestinationASupprimer
      		TableAjouteLigne(TableListe,Vrai,sFichierEnCours,CST_FICHIER_A_SUPPRIMER)
      	FIN
      	
      FIN
      
      // On mémorise le nombre de fichiers à copier et à supprimer
      gnFicACopier = COL_Synchronisation.gtabFichierDestinationACopier..Occurrence
      gnFicASupprimer = COL_Synchronisation.gtabFichierDestinationASupprimer..Occurrence
     type : 458752
   -
     name : RemplirTableComparaison
     procedure_id : 1741515350552492451
     type_code : 14
     code : |1+
      // Résumé : Remplit la table avec l'analyse de synchronisation
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RemplirTableComparaison()
      
      // Variables locales
      tabRepertoireException est un tableau de chaine
      tabExtensionsException est un tableau de chaine
      
      SI ListeOccurrence(Combo_PROJET)<=0 ALORS RETOUR
      
      // Début du remplissage de la table
      TableDébutRemplissage(TableListe)
      
      // Lecture des exceptions
      SI PROJET.RépertoiresException<>"" alors
      	ChaîneVersTableau(PROJET.RépertoiresException,tabRepertoireException,RC)
      FIN
      
      SI PROJET.FichiersException<>"" alors
      	ChaîneVersTableau(PROJET.FichiersException,tabExtensionsException,RC)
      Fin
      
      // Configure les exceptions
      COL_Synchronisation.DefinirException.tabListeRepertoireException =tabRepertoireException
      COL_Synchronisation.DefinirException.tabListeExtensionsException =tabExtensionsException
      COL_Synchronisation.DefinirException()
      
      // on calcule la synchronisation des deux répertoires	
      COL_Synchronisation.AnalyseRépertoires.sNomRépertoireSource = SAI_REPERTOIRE_SOURCE
      COL_Synchronisation.AnalyseRépertoires.sNomRépertoireDestination = SAI_REPERTOIRE_DESTINATION
      COL_Synchronisation.AnalyseRépertoires.bAnalyserSousRepertoire = gbSousRep
      COL_Synchronisation.AnalyseRépertoires.bEcraserFichiersPlusRécents = gbEcrase
      SI COL_Synchronisation.AnalyseRépertoires() = Faux ALORS
      	Erreur("La comparaison ne peut pas être effectuée",ErreurInfo())
      FIN
      
      // On affiche le résultat de la synchronisation proposée
      AfficheComparaison()
      
      // Fin du remplissage de la table
      TableFinRemplissage(TableListe)
      
     type : 458752
